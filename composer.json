{
  "name": "fiedsch/tokenlogin-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 Token Login Bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Fieger",
      "homepage": "https://www.github.com/fiedsch/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "contao/core-bundle": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Fiedsch\\TokenloginBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/assets",
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/templates"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Fiedsch\\TokenloginBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
